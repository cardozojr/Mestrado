@ARTICLE{Blaschke2020,
	author = {Blaschke, Lisa Marie and Marín, Victoria I.},
	title = {Applications of heutagogy in the educational use of e-portfolios; [Aplicaciones de la heutagogía en el uso educativo de e-portfolios]},
