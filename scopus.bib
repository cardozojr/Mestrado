}

@ARTICLE{Stoten2024991,
	author = {Stoten, David William},
	title = {Positioning through epistemic cognition in higher education: conceptualising the ways in which academics in a business school view heutagogy},
	year = {2024},
	journal = {Higher Education},
	volume = {87},
	number = {4},
	pages = {991 – 1007},
	doi = {10.1007/s10734-023-01048-2},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85159269034&doi=10.1007%2fs10734-023-01048-2&partnerID=40&md5=fd8bbf2d888ff446a3685cca684e214f},
	affiliations = {Newcastle Business School, Northumbria University, Newcastle Upon Tyne, United Kingdom},
	abstract = {Teaching and learning in higher education is informed by a multitude of conditioning factors, not least the values systems and outlook of academics. Understanding the epistemological positioning taken by academics in relation to teaching and learning is therefore important if we are to make judgments about how we educate now, and could do so in the future. Developments in educational theory and digital technology have opened-up new possibilities for the ways in which students learn, and to a degree this has been accelerated by the responses from universities to the COVID-19 pandemic. How then should we conceive the future? Heutagogy is one of a number of theoretical approaches that has attracted interest from those who wish to see greater student control over the learning journey- but how widespread is this view amongst academics? This paper reports on a qualitative study in which 12 academics in an English Business School were asked to describe their views on teaching and learning, which we can encapsulate through the concept of epistemic cognition. The findings infer that there is little epistemological underpinning for heutagogy and that if academics are going to innovate, then additional resource and professional development should be put in place to support epistemic reflexivity, and a shift in their epistemological positioning. The paper conceptualises academics’ positioning through a typology of epistemic views. © The Author(s), under exclusive licence to Springer Nature B.V. 2023.},
	author_keywords = {Business school; Epistemic cognition; Epistemic competence; Heutagogy; Teaching and learning},
	correspondence_address = {D.W. Stoten; Newcastle Business School, Northumbria University, Newcastle Upon Tyne, United Kingdom; email: david.stoten@northumbria.ac.uk},
	publisher = {Springer Science and Business Media B.V.},
	issn = {00181560},
	language = {English},
	abbrev_source_title = {High. Educ.},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 2}
}

@BOOK{Mohamad2024210,
	author = {Mohamad, M.M. and Mafarja, N. and Affendi, H. Mohd and Ali, R.H. Raja Mohd. and Ahmad, A.},
	title = {The concept of higher education teaching design toward future TVET flexible learning an analysis of Heuta-Peera-Cyber approaches},
	year = {2024},
	journal = {Innovative Pedagogical Practices for Higher Education 4.0: Solutions and Demands of the Modern Classroom},
	pages = {210 – 229},
	doi = {10.1201/9781003400691-13},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85204580474&doi=10.1201%2f9781003400691-13&partnerID=40&md5=a1c35357efbf03e515bdc0040f80fcf1},
	affiliations = {Department of Professional Education and Postgraduate, Faculty of Technical and Vocational Education, Universiti Tun Hussein Onn Malaysia, Batu Pahat, Malaysia; Faculty of Build Environment, Universiti Kebangsaan Malaysia, Malaysia; Department of Audit and Information Systems, Universiti Utara Malaysia, Malaysia; School of Education, Faculty of Social Sciences and Humanities, Universiti Teknologi Malaysia, Malaysia},
	abstract = {Technical and Vocational Education and Training (TVET) pedagogy refers to principles, methods, and practices used in teaching and learning vocational skills. TVET is focused on preparing individuals for the world of work by acquiring practical skills and knowledge. In recent years, pedagogical approaches toward flexible learning have been integrated. The teaching design in higher education nowadays enhances learning outcomes and prepares learners for the 21st-century workplace. Three pedagogical approaches were selected in designing the concept of TVET teaching strategy and integrated based on heutagogy, peeragogy, and cybergogy. Heutagogy encourages self-directed learning, while peeragogy fosters collaboration and learning from each other. Cybergogy uses digital technology to integrate TVET education, equipping students with knowledge, skills, and attitudes to thrive in the digital age and prepare them for future jobs. The concept of Heuta-Peera-Cyber was proposed after careful analysis of literature and past research. The result shows the principles of heutagogy can be linked with collaboration with peers and implementing technology to improve and develop learning experiences. In summary, by providing the resources for learning, the concept of teaching design can help to invent a more engaging, effective, and enjoyable learning experience. © 2025 selection and editorial matter, Muhammad Mujtaba Asad, Prathamesh Padmakar Churi, Fahad Sherwani, and Razali Bin Hassan. All rights reserved.},
	publisher = {CRC Press},
	isbn = {978-104011976-1; 978-103250859-7},
	language = {English},
	abbrev_source_title = {Innov. Pedagog. Pract. for High. Educ. 4.0: Solut. and Demands of the Mod. Classr.},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0}
}